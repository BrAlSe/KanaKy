# this import will prevent disappear tabs through some clicks on them)))
#:import md_icons kivymd.icon_definitions.md_icons
#:import fonts kivymd.font_definitions.fonts

# Пункт меню в списке DrawerList.
<ItemDrawer>
    theme_text_color: "Custom"
    on_release: self.parent.set_color_item(self)

    IconLeftWidget:
        id: icon
        icon: root.icon
        theme_text_color: "Custom"
        text_color: root.text_color
 
<ContentNavigationDrawer>
    orientation: "vertical"
    padding: "8dp"
    spacing: "8dp"

    AnchorLayout:
        size_hint_y: None
        height: avatar.height

        Image:
            id: avatar
            size_hint: None, None
            size: "100dp", "100dp"
            source: "logo.png" # прописать путь к картинке
			pos_hint: {"center_x": .5}

    MDLabel:
        text: "Кафе на Кузнецова" #заголовок
        font_style: "Button"
        size_hint_y: None
        height: self.texture_size[1]
		halign: "center"

    MDLabel:
        text: app.by_who 
        font_style: "Caption"
        size_hint_y: None
        height: self.texture_size[1]
		halign: "center"

    ScrollView:

        MDList:

            OneLineIconListItem:
                text: "Пересчёт"
				on_press:
                    root.nav_drawer.set_state("close")
                    root.screen_manager.current = "Пересчёт"
				IconLeftWidget:
					icon: 'calculator'


            OneLineIconListItem:
                text: "Купюроподсчёт"
                on_press:
                    root.nav_drawer.set_state("close")
                    root.screen_manager.current = "Купюроподсчёт"
				IconLeftWidget:
					icon: 'account-cash-outline'
    
            OneLineIconListItem:
                text: "Сводка"
                on_press:
                    root.nav_drawer.set_state("close")
                    root.screen_manager.current = "сводка"
				IconLeftWidget:
					icon: 'dock-top'

            OneLineIconListItem:
                text: "Конверты"
                on_press:
                    root.nav_drawer.set_state("close")
                    root.screen_manager.current = "конверты"
				IconLeftWidget:
					icon: 'email-newsletter'
    
            OneLineIconListItem:
                text: "Поставщики"
                on_press:
                    root.nav_drawer.set_state("close")
                    root.screen_manager.current = "Поставщики"
				IconLeftWidget:
					icon: 'card-account-phone'

            OneLineIconListItem:
                text: "пора на море"
                on_press:
                    root.nav_drawer.set_state("close")
                    root.screen_manager.current = "море"
				IconLeftWidget:
					icon: 'beach'
    
